{
    "name":         "antoinelemaire/blacklist-bundle",
    "description":  "Symfony2 bundle for blacklist emails.",
    "keywords":     ["blacklist","email"],
    "homepage":     "https://github.com/AntoineLemaire/BlacklistBundle",
    "type":         "symfony-bundle",
    "license":      "MIT",
    "version":      "0.1",
    "authors": [
        {
            "name":   "Antoine Lemaire",
            "email":  "lemaireantoine@hotmail.com"
        }
    ],
    "require": {
        "php": "^5.5.0 || ^7.0",
        "symfony/framework-bundle": "^3.3 || ^4.0",
        "symfony/debug": "^3.0 || ^4.0",
        "doctrine/common": "^2.3",
        "symfony/validator": "^3.3 || ^4.0",
        "knplabs/doctrine-behaviors": "~1.4",
